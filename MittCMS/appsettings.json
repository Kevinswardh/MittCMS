{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "ad9d3928-2f93-4efc-9ebd-b765393bb6e9",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Runtime": {
        "Mode": "Production"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },

  "ConnectionStrings": {
    "umbracoDbDSN": "@Microsoft.KeyVault(SecretUri=https://mittcms-keys.vault.azure.net/secrets/Umbraco--ConnectionStrings--umbracoDbDSN/)",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}
